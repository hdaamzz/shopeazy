           <script>
            document.getElementById('CategoryListForm').addEventListener('submit', async function(event) {
                event.preventDefault();
            
                // Form data validation code (omitted for brevity)
                let isValid = true;

                    const categoryTitle = document.getElementById('ecommerce-category-title');
                    if (!categoryTitle.value.trim()) {
                        categoryTitle.classList.add('is-invalid');
                        isValid = false;
                    } else {
                        categoryTitle.classList.remove('is-invalid');
                    }

                    // Validate description
                    const description = document.getElementById('description');
                    if (!description.value.trim() || description.value.length < 10 || description.value.length > 200) {
                        description.classList.add('is-invalid');
                        isValid = false;
                    } else {
                        description.classList.remove('is-invalid');
                    }

                    // Validate category status
                    const categoryStatus = document.getElementById('ecommerce-category-status');
                    if (!categoryStatus.value) {
                        categoryStatus.classList.add('is-invalid');
                        isValid = false;
                    } else {
                        categoryStatus.classList.remove('is-invalid');
                    }
            
                if (isValid) {
                    const formData = {
                        categoryTitle: categoryTitle.value.trim(),
                        description: description.value.trim(),
                        categoryoption: categoryStatus.value
                    };
            
                    try {
                        const response = await fetch('/admin/category', {
                            method: 'POST',
                            headers: {
                                'Content-Type': 'application/json'
                            },
                            body: JSON.stringify(formData)
                        });
                        const data = await response.json();
                        if (data.success) {
                                        window.location.href = data.redirectUrl;
                                    } else {
                                        Swal.fire({
                                            title: 'Error!',
                                            text: data.message,
                                            icon: 'error',
                                            confirmButtonText: 'OK'
                                        });
                                    }
                    } catch (error) {
                        console.error('Error:', error);
                    }
                }
            });
        </script>
    

