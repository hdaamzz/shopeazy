            <script>
                //search field
                document.getElementById("search").addEventListener('keyup', function () {
                    var filter = this.value.toLowerCase();
                    var rows = document.querySelectorAll('table tr');

                    rows.forEach(function (row) {
                        var text = row.textContent.toLowerCase();
                        row.style.display = text.includes(filter) ? '' : 'none';
                    });
                });


                //block and unblock
                document.addEventListener('DOMContentLoaded', () => {
                    const blockForms = document.querySelectorAll('.blockForm');

                    blockForms.forEach(form => {
                        form.addEventListener('submit', async (event) => {
                            event.preventDefault();
                            const userId = form.getAttribute('data-user-id');
                            await handleBlockUnblock(userId, form);
                        });
                    });
                });

                const handleBlockUnblock = async (userId, form) => {
                    try {
                        const response = await fetch(`/admin/api/users/block/${userId}`, {
                            method: 'POST',
                            headers: {
                                'Content-Type': 'application/json',
                            }
                        });

                        const blockData = await response.json();
                        if (blockData.success) {
                            // Success SweetAlert
                            await Swal.fire({
                                icon: 'success',
                                title: 'Success!',
                                text: blockData.message,
                                confirmButtonColor: '#3085d6',
                            });

                            // Update the button text and class
                            const button = form.querySelector('button');
                            if (button.textContent === 'BLOCK') {
                                button.textContent = 'UNBLOCK';
                                button.className = 'button1';
                            } else {
                                button.textContent = 'BLOCK';
                                button.className = 'button2';
                            }
                        } else {
                            // Error SweetAlert
                            await Swal.fire({
                                icon: 'error',
                                title: 'Oops...',
                                text: blockData.message,
                                confirmButtonColor: '#d33',
                            });
                        }
                    } catch (error) {
                        console.error('Error:', error);
                        // Error SweetAlert for unexpected errors
                        await Swal.fire({
                            icon: 'error',
                            title: 'Oops...',
                            text: 'An error occurred while processing your request',
                            confirmButtonColor: '#d33',
                        });
                    }
                }
            </script>

