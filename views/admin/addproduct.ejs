            <script>

                    function previewImage(input, previewId) {
                    const preview = document.getElementById(previewId);
                    const file = input.files[0];
                    const reader = new FileReader();

                    reader.onloadend = function () {
                        preview.src = reader.result;
                        preview.style.display = 'block';
                        input.closest('.image-upload-container').querySelector('.upload-placeholder').style.display = 'none';
                    }

                    if (file) {
                        reader.readAsDataURL(file);
                    } else {
                        preview.src = "";
                        preview.style.display = 'none';
                        input.closest('.image-upload-container').querySelector('.upload-placeholder').style.display = 'flex';
                    }
                }

                function triggerInputClick(inputId) {
                    document.getElementById(inputId).click();
                }



             
                document.addEventListener('DOMContentLoaded', function() {
                    const addProductForm = document.getElementById('addProductForm');
                    const name = document.getElementById('ecommerce-product-name');
                    const description = document.getElementById('ecommerce-product-description');
                    const image1 =document.getElementById('productImage1');
                    const image2 =document.getElementById('productImage2');
                    const image3 =document.getElementById('productImage3');
                    const price = document.getElementById('ecommerce-product-price');
                    const stock = document.getElementById('productCount');
                    const status =document.getElementById('product-status');
                    const category =document.getElementById('ecommerce-category');
                    const checkk1 =document.getElementById('diii1')
                    const checkk2 =document.getElementById('diii2')
                    const checkk3 =document.getElementById('diii3')

                    

                    addProductForm.addEventListener('submit', function (event) {
                        event.preventDefault();
                        
                        
                        const nameValue = name.value.trim();
                        const descriptionValue = description.value.trim();
                        const priceValue = price.value.trim();
                        const stockValue = stock.value.trim();
                        const image1value = image1.files.length;
                        console.log(image1value)
                        const image2value = image2.files.length;
                        const image3value = image3.files.length;
                        const categoryValue = category.value.trim();
                        const statusValue = status.value.trim();
                        console.log(statusValue);
                        const file1 = image1.files[0];
                        const file2 = image2.files[0];
                        const file3 = image3.files[0];
                        const fileTypes = [file1, file2, file3].map(file => file ? file.type : '');
                        
                        

                        validateRegisterForm(nameValue,descriptionValue,priceValue,stockValue,image1value,image2value,image3value,statusValue,categoryValue,...fileTypes);
                     });
                     const setError = (element, message) => {
                            const formGroup1 = element.parentElement;
                            const errorDisplay = formGroup1.querySelector('.errMsgSignup');
                            if (errorDisplay) {
                                errorDisplay.innerText = message;
                                element.classList.add('error');
                                errorDisplay.classList.add('error');
                                element.classList.remove('success');
                                errorDisplay.classList.remove('success');
                            }
                        }

                        const setSuccess = (element, message) => {
                            const formGroup1 = element.parentElement;
                            const errorDisplay = formGroup1.querySelector('.errMsgSignup');
                            if (errorDisplay) {
                                errorDisplay.innerText = message;
                                element.classList.add('success');
                                errorDisplay.classList.add('success');
                                element.classList.remove('error');
                                errorDisplay.classList.remove('error');
                            }
                        }

                    const validateRegisterForm = async (nameValue, descriptionValue, priceValue, stockValue ,image1value,image2value,image3value,statusValue,categoryValue,file1Type, file2Type, file3Type) => {
                        let isValid = true;

                            if (nameValue === '') {
                                setError(name, 'Enter Product Name');
                                isValid = false;
                            } else if (nameValue.length < 5) {
                                setError(name, 'Please Enter Correct Product Name');
                                isValid = false;
                            } else {
                                setSuccess(name, 'Product Name');
                            }
                            if (descriptionValue === '') {
                                setError(description, 'Enter Description');
                                isValid = false;
                            } else {
                                setSuccess(description, 'Description');
                            }

                            if (priceValue === '') {
